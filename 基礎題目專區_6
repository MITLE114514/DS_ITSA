#pragma warning( disable: 6031 4996 )
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>

int main() {
    int input;
    while(scanf("%d", &input) != EOF) {
        if(input >= 3 && input <= 5) {
            printf("Spring\n");
        }
        else if(input >= 6 && input <= 8) {
            printf("Summer\n");
        }
        else if(input >= 9 && input <= 11) {
            printf("Autumn\n");
        }
        else {
            printf("Winter\n");
        }
    }
}
